@model IEnumerable<Inspinia_MVC5.Models.GetIncreaseOnWashes_Result>

<table id="table-increases" class="display table table-striped table-bordered table-hover responsive">
    <thead>
        <tr>
            <th>Регион</th>
            <th>ID</th>
            <th>Название</th>
            <th>Адрес</th>
            <th>Сумма</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var n in Model)
        {
            <tr>
                <td>@n.region</td>
                <td>
                    <button id="clear-date" type="button" onclick="selectPost(@n.IDWash)" class="btn btn-lg btn-outline-info">@n.IDWash</button>
                </td>
                <td>@n.washName</td>
                <td>@n.washAddress</td>
                <td>@n.amount</td>
            </tr>
        }
    </tbody>
</table>
