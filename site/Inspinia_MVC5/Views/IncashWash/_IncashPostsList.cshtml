@model IEnumerable<Inspinia_MVC5.Models.GetPostAmounts_Result>

<table class="table table-striped table-bordered table-hover table_incashposts">
    <thead>
        <tr>
            <th>Пост</th>
            <th>Зачисления</th>
            <th data-hide="phone">М10 шт</th>
            <th data-hide="phone">Б10 шт</th>
            <th data-hide="phone">Б100 шт</th>
            <th data-hide="phone">Б500 шт</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {
        <tr>
            <td>@i.Name</td>
            <td>@i.amount</td>
            <td>@i.m10</td>
            <td>@i.b10</td>
            <td>@i.b50</td>
            <td>@i.b100</td>
        </tr>
        }
    </tbody>
    @*<tfoot>
            <tr>
                <td colspan="7">
                    <ul class="pagination float-right"></ul>
                </td>
            </tr>
        </tfoot>*@
</table>
