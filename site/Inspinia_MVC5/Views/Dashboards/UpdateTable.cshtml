@foreach (var o in ViewBag.Operations)
    {
        <tr>
            <td><p>@o.OperationID</p></td>
            <td><p>@o.PscID</p></td>
            <td><p>@o.OperationTypeID</p></td>
            <td><p>@o.CardID</p></td>
            <td><p>@o.DTime</p></td>
            <td><p>@o.Amount</p></td>
            <td><p>@o.Balance</p></td>
            <td><p>@o.ServerID</p></td>
        </tr>
    }
