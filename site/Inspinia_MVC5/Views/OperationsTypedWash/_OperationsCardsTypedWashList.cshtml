@model IEnumerable<Inspinia_MVC5.Models.GetCardList_Result>

<table id="table-typed-wash" class="table table-striped table-bordered table-hover table_incashwashes">
    <thead>
        <tr>
            <th>Время операции</th>
            <th>Номер карты</th>
            <th>Операция</th>
            <th>Место</th>
            <th>Функции</th>
            <th>Подробности</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@m.ActivationDate</td>
                <td>@m.CardNum</td>
                <td>@m.CardTypeName</td>
                <td>@m.ActivationBy</td>
                <td>@m.CardStatusName</td>
                <td>@m.DecreaseSum</td>
            </tr>
        }
    </tbody>
</table>
