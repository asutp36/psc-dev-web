@model IEnumerable<Inspinia_MVC5.Models.GetFinanceList_Result>

<table id="table-finance" class="table table-striped table-bordered table-hover table_incashwashes">
    <thead>
        <tr>
            <th>Дата</th>
            <th>Тип операции</th>
            <th>Сумма</th>
            <th>Устройство</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
        <tr>
            <td>@m.DTime</td>
            <td>@m.Name</td>
            <td>@m.Amount</td>
            <td>Макет</td>
        </tr>
        }
    </tbody>
</table>

